{
  "name": "create-react-components",
  "version": "1.0.2",
  "description": "",
  "author": "Tim Tawan <ttawan@github.com>",
  "main": "lib/index.js",
  "style": "dist/styles.css",
  "scripts": {
    "clean": "rm -rf dist lib coverage",
    "start": "NODE_ENV=development start-storybook -p 9000 --config-dir config/storybook",
    "dev": "NODE_ENV=development rollup -c config/rollup/rollup.config.js -w",
    "build": "NODE_ENV=development npm run clean && rollup -c config/rollup/rollup.config.js",
    "build:production": "NODE_ENV=production npm run clean && npm run check && rollup -c config/rollup/rollup.config.production.js",
    "check": "npm run test && npm run lint",
    "lint": "eslint src/ --fix",
    "test": "NODE_ENV=development jest",
    "test:watch": "jest --watchAll",
    "coverage": "npm run test:coverage & npm run typecheck:coverage",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "styled-components": "^3.2.5"
  },
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@storybook/addon-info": "^3.1.6",
    "@storybook/addon-knobs": "^3.1.8",
    "@storybook/addon-notes": "^3.1.6",
    "@storybook/react": "^3.1.8",
    "autoprefixer": "^7.1.2",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-inline-react-svg": "^0.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "classnames": "^2.2.5",
    "cssnano": "^3.10.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "expect-enzyme": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^3.5.3",
    "livereload": "^0.6.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-global-import": "^1.0.1",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-modules": "^0.8.0",
    "postcss-nested": "^2.1.0",
    "postcss-simple-vars": "^4.0.0",
    "postcss-url": "^7.1.1",
    "precss": "^2.0.0",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-image": "^1.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.4",
    "rollup-plugin-replace": "^1.2.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-url": "^1.3.0",
    "rollup-watch": "^4.3.1",
    "svg-react-loader": "^0.4.4",
    "webpack": "^3.4.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "plugin:jest/recommended"
    ],
    "plugins": [
      "jest"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "semi": 0,
      "react/require-default-props": 0,
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/anchor-is-valid": [
        "warn",
        {
          "aspects": [
            "invalidHref"
          ]
        }
      ]
    },
    "env": {
      "jest/globals": true
    }
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/fileMock.js"
    }
  }
}
